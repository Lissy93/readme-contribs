{
  "name": "readme-contribs",
  "version": "2.0.0",
  "description": "GitHub README embed badges for contributors and sponsors",
  "author": "Alicia Sykes <alicia@omg.com> (https://aliciasykes.com)",
  "license": "MIT",
  "homepage": "https://readme-contribs.as93.net/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lissy93/readme-contribs.git"
  },
  "bugs": {
    "url": "https://github.com/Lissy93/readme-contribs/issues"
  },
  "main": "./api/server.ts",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "private": false,
  "scripts": {
    "dev": "bun run server.ts",
    "dev:vercel": "vercel dev",
    "start": "bun run server.ts",
    "deploy": "vercel",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "quality": "biome check .",
    "quality:fix": "biome check --write ."
  },
  "dependencies": {
    "@hono/zod-validator": "^0.7.6",
    "@resvg/resvg-js": "^2.6.2",
    "hono": "^4.4.7",
    "zod": "^4.3.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@vitest/coverage-v8": "^4.0.16",
    "vercel": "^32.4.1",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
